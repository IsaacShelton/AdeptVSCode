{
    "name": "adept-language",
    "displayName": "Adept Language",
    "description": "Adept Language Support for VS Code",
    "version": "1.8.0",
    "publisher": "DockySoft",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/IsaacShelton/AdeptVSCode/"
    },
    "icon": "icon.png",
    "galleryBanner": {
        "color": "0x62b7d8",
        "theme": "light"
    },
    "contributes": {
        "languages": [
            {
                "id": "adept",
                "aliases": [
                    "Adept",
                    "adept"
                ],
                "extensions": [
                    ".adept"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "adept",
                "scopeName": "source.adept",
                "path": "./syntaxes/adept.tmLanguage.json"
            }
        ]
    }
}
